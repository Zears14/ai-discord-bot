{
  "name": "zears-ai-h",
  "version": "1.0.0",
  "description": "Discord bot with AI text and image generation capabilities",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.js",
    "dev": "NODE_ENV=development bun run --watch src/index.js",
    "lint": "bunx eslint . --ext .js,.mjs",
    "format": "bunx prettier --check .",
    "fix": "bun run lint --fix && bun prettier --write .",
    "check": "bun run lint && bun run format",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/genai": "^0.13.0",
    "debug": "^4.4.3",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.5",
    "pg": "^8.16.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "prettier": "^3.3.3",
    "prettier-plugin-sql": "^0.19.2",
    "husky": "^9.1.7"
  },
  "engines": {
    "bun": ">=1.1.0"
  }
}
